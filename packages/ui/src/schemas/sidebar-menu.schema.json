{
  "id": "sidebar-menu",
  "name": "Sidebar Menu",
  "category": "Navigation",
  "icon": "sidebar-menu",
  "acceptsChildren": false,
  "properties": {
    "navigationMenu": {
      "type": "enum",
      "values": ["none", "header", "footer", "sidebar", "mobile"],
      "default": "sidebar",
      "label": "Navigation Menu"
    },
    "items": {
      "type": "navitems",
      "label": "Menu Items",
      "default": []
    },
    "showIcons": {
      "type": "boolean",
      "default": false,
      "label": "Show Icons"
    },
    "collapsibleSections": {
      "type": "boolean",
      "default": true,
      "label": "Collapsible Sections"
    },
    "defaultExpanded": {
      "type": "enum",
      "values": ["all", "active-branch", "none"],
      "default": "all",
      "label": "Default Expanded State"
    },
    "sticky": {
      "type": "boolean",
      "default": false,
      "label": "Sticky Position"
    }
  },
  "responsive": {
    "breakpoints": ["desktop", "tablet", "mobile"],
    "stackBehavior": "collapsible-on-mobile"
  }
}
